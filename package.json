{
  "name": "printbroker",
  "version": "1.0.0",
  "description": "Print Broker - Baskı hizmetleri için aracı platform",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "start": "node server/index.js",
    "db:push": "drizzle-kit push:pg",
    "build:frontend": "vite build --config client/vite.config.ts",
    "build:backend": "tsc -p tsconfig.server.json",
    "push:frontend": "git add . && git commit -m \"frontend: update\" && git push origin main",
    "push:backend": "git add . && git commit -m 'backend: update' && git push origin main",
    "deploy:frontend": "npm run build:frontend && npm run push:frontend",
    "deploy:backend": "npm run build:backend && npm run push:backend",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "test:coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.4",
    "@neondatabase/serverless": "^1.0.1",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-toast": "^1.2.14",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@tanstack/react-query": "^5.24.1",
    "bcryptjs": "^2.4.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.29.3",
    "express": "^4.18.3",
    "framer-motion": "^12.18.1",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.515.0",
    "pg": "^8.11.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.50.1",
    "tailwind-merge": "^3.3.1",
    "wouter": "^3.7.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.13",
    "@types/node": "^20.11.24",
    "@types/pg": "^8.11.2",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@types/supertest": "^6.0.3",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.21",
    "drizzle-kit": "^0.31.1",
    "jest": "^30.0.0",
    "nodemon": "^3.1.0",
    "postcss": "^8.4.35",
    "rimraf": "^5.0.5",
    "supertest": "^7.1.1",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.4.0",
    "typescript": "^5.3.3",
    "vite": "^5.1.3"
  }
